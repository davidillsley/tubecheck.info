{
  "manifest_version": 2,
  "name": "tubecheck.info",
  "version": "0.6",

  "description": "Shows current status of the London tube network",
  "icons": {
    "48": "data/tube.png"
  },

  "browser_action": {
    "default_icon": "data/tube.png",
    "default_title": "Tube Status",
    "default_popup": "data/index.html"
  }
}
